# Copy this file to .env.local and fill in your actual values

# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/movie-list

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here-generate-a-random-string

# Google OAuth (Get from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Admin Configuration (Set to your Google email(s) for admin access)
# For multiple admins, use ADMIN_EMAILS with comma-separated emails
ADMIN_EMAILS=admin@example.com,poornikavanta06@gmail.com,sudeepthi924@gmail.com
# Legacy single admin support (optional if using ADMIN_EMAILS)
ADMIN_EMAIL=admin@example.com

# AWS S3 Configuration (for image uploads)
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=ap-south-2
